{
  "name": "@tripie/design-system",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./my-button": "./src/my-button.tsx",
    ".": [
      "./src/index.ts",
      "./src/index.tsx"
    ],
    "./*": [
      "./src/*.tsx",
      "./src/*.ts",
      "./src/*/index.ts"
    ],
    "./*/*": [
      "./src/*/*.tsx",
      "./src/*/*.ts"
    ]
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --max-warnings 0",
    "generate:component": "turbo gen react-component",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/preset-react": "^7.24.7",
    "babel-jest": "^29.7.0",
    "@jest/globals": "^29.7.0",
    "@storybook/react": "^8.1.11",
    "@testing-library/react": "^16.0.0",
    "@tripie/eslint-config": "workspace:*",
    "@tripie/typescript-config": "workspace:*",
    "@turbo/gen": "^1.12.4",
    "@types/eslint": "^8.56.5",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "csstype": "^3.1.3",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-scss-transform": "^1.0.3",
    "node-sass": "^9.0.0",
    "ts-jest": "^29.2.3",
    "typescript": "^5.3.3",
    "@tripie/jest-config": "workspace:*"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "next": "15.0.0-rc.0",
    "postcss": "^8",
    "react": "^18.2.0",
    "usehooks-ts": "^3.1.0"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*",
        "src/*/index"
      ]
    }
  }
}
